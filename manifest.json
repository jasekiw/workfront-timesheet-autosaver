{
  "manifest_version": 2,

  "name": "Wokrfront Timesheet Autosaver",
  "description": "This extension will autosave your timesheet",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "tabs",
    "webNavigation",
    "*://*.my.workfront.com/*"
  ]
}